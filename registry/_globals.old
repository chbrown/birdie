# this file will be read as a snippet immediately preceding any other registry yaml file
# this allows other yaml files to use the &anchors defined in this file by their *alias
global:
  - &github https://raw.github.com
  - &cdnjs https://cdnjs.cloudflare.com/ajax/libs
  - &google https://ajax.googleapis.com/ajax/libs
  - &ms http://ajax.aspnetcdn.com


# this file will be read as a snippet immediately preceding any other registry yaml file
# this allows other yaml files to use the &anchors defined in this file by their *alias
global:
  - &http
    root: http://
  - &https
    root: https://
  - &github
    root: https://raw.github.com/
  - &cdnjs
    root: https://cdnjs.cloudflare.com/ajax/libs/
  - &google
    root: https://ajax.googleapis.com/ajax/libs/
  - &ms
    root: http://ajax.aspnetcdn.com/
